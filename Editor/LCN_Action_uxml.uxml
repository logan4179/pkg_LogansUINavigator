<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="True">
    <ui:VisualElement style="background-color: rgb(96, 94, 94); padding-left: 8px; padding-right: 8px; padding-top: 8px; padding-bottom: 8px; margin-top: 5px; margin-bottom: 5px; border-left-color: rgb(195, 195, 195); border-right-color: rgb(195, 195, 195); border-top-color: rgb(195, 195, 195); border-bottom-color: rgb(195, 195, 195); border-left-width: 2px; border-right-width: 2px; border-top-width: 2px; border-bottom-width: 2px;">
        <ui:TextField picking-mode="Ignore" text="Place description here..." binding-path="description" style="margin-top: 5px; -unity-font-style: bold; font-size: 14px; margin-bottom: 5px;" />
        <uie:PropertyField name="PropertyField_Manager" binding-path="_mgr" label="Manger:" />
        <uie:PropertyField name="PropertyField_MyListener" binding-path="myListener" label="My Listener:" />
